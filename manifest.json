{
    "name": "Page Filter",
    "description": "Adds and removes filters from the active tab",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "omnibox": { "keyword" : "tint" },
    "icons": {
      "16": "window.jpg"
    },
    "background": {
      "scripts": ["backgroundtest.js"],
      "persistent": false
    },
    "browser_action": {
      "default_title": "tint the page"
    },
    "manifest_version": 2
}
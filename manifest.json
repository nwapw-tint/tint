{
    "name": "Page Filter",
    "description": "Adds and removes filters from the active tab",
    "version": "1.0",
    "permissions": [
      "background",
      "tabs", 
      "http://*/*",
      "https://*/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://*/*"],
        "js": ["promptAdd.js"],
        "run_at": "document_start"
      }
    ],
    "omnibox": { "keyword" : "tint" },
        "icons": {
          "16": "window.jpg"
        },
    "browser_action": {
      "default_title": "tint the page"
    },
    "manifest_version": 2
}